<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="mrp_routing_workcenter_form_view_pdf_workorder" model="ir.ui.view">
            <field name="name">mrp.routing.workcenter.form.pdf.workorder</field>
            <field name="model">mrp.routing.workcenter</field>
            <field name="inherit_id" ref="mrp.mrp_routing_workcenter_form_view"/>
            <field name="arch" type="xml">
            	<xpath expr="//field[@name='worksheet_type']" position="after">
                    <field name="use_plm_pdf"/>
            	</xpath>
            </field>
        </record>  

    </data>

</odoo>
